{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "removeComments": true,
    "sourceMap": false,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true
  }
}